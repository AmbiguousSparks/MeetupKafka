{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Mongo": "mongodb://root:root@localhost:27017/"
  },
  "ConsumerConfig": {
    "BootstrapServers": "host.docker.internal:29092",
    "GroupId": "new.product"
  },
  "ProducerConfig": {
    "BootstrapServers": "host.docker.internal:29092",
    "MessageTimeoutMs": 150000,
    "MessageSendMaxRetires": 3
  }
}
